import java.util.Arrays;

public class Main {
    public static void main(String[] args) {
        int[] arr = {0, 1, 2};
        int deltaElement = 5;
        for (int i = 0; i < arr.length; i++) {
            arr[i] += deltaElement;
        }
        System.out.print(Arrays.toString(arr));
    }
}
